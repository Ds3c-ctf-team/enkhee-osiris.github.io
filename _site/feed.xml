      &lt;td&gt;&lt;code class=&quot;highlighter-rouge&quot;&gt;#ffffff&lt;/code&gt;&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;Ногоон&lt;/td&gt;
      &lt;td&gt;&lt;code class=&quot;highlighter-rouge&quot;&gt;#25ba84&lt;/code&gt;&lt;/td&gt;
    &lt;/tr&gt;
